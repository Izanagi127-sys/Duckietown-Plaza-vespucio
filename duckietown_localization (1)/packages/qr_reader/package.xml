<?xml version="1.0"?>
<package format="2">
  <name>qr_reader</name>
  <version>0.0.1</version>
  <description>QR reader node that reacts to messages from vision_superior_node</description>

  <maintainer email="tucorreo@ejemplo.com">Tu Nombre</maintainer>
  <license>MIT</license>

  <buildtool_depend>catkin</buildtool_depend>

  <build_depend>rospy</build_depend>
  <build_depend>std_msgs</build_depend>
